{
  "name": "right",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --open",
    "generate:readme": "npx marked -o readme.html -i README.md",
    "generate:manifest-icons": "pwa-asset-generator public/app-icon.png public/icons --favicon -i ./index.html",
    "lint": "eslint src --ext ts,tsx",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\"",
    "start": "vite",
    "preview": "vite preview",
    "test": "playwright test -c playwright.config.ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "commander": "^12.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "9.0.11",
    "prettier": "^3.3.2",
    "prettier-plugin-pkg": "0.18.1",
    "prettier-plugin-sh": "^0.14.0",
    "pwa-asset-generator": "^6.3.1",
    "replace-in-file": "^8.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "vite-plugin-pwa": "^0.20.0"
  }
}
